{
  "name": "jasperfw/jasperfw",
  "description": "A simple but powerful MVC framework developed to allow developers to get sites up and running quickly",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "William Eldridge",
      "homepage": "http://wigedev.net"
    }
  ],
  "require": {
    "ext-json": "*",
    "jasperfw/core": "dev-master",
    "php": ">=7.2",
    "psr/container": "1.0.*",
    "psr/log": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "mikey179/vfsstream": "1.6.8"
  },
  "suggest": {
    "twig/twig": "If you will be using the TWIG template engine (^3.0)"
  },
  "autoload": {
    "psr-4": {
      "JasperFW\\JasperFW\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JasperFW\\JasperFWTests\\": "tests/"
    }
  }
}
